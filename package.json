{
  "name": "deploy_test",
  "version": "1.0.0",
  "description": "deploy test",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config build-utils/webpack.config.js --env env=prod",
    "build:analyze": "npm run build -- --env addon=bundleanalyze",
    "lint": "eslint  --ignore-path .gitignore --fix .",
    "start": "webpack serve --config build-utils/webpack.config.js --env env=dev",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-all": "jest",
    "ncuu": "ncu -u",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [],
  "author": "Deploy test",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "axios": "0.21.1",
    "core-js": "~3.13.0",
    "gh-pages": "3.2.0",
    "history": "5.0.0",
    "lodash": "4.17.21",
    "luxon": "^1.27.0",
    "prop-types": "~15.7.2",
    "react": "~17.0.2",
    "react-csv": "2.0.3",
    "react-dom": "~17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "~0.13.7"
  },
  "devDependencies": {
    "@babel/core": "~7.14.3",
    "@babel/eslint-parser": "~7.14.4",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "~7.14.4",
    "@babel/preset-react": "~7.13.13",
    "@jest/globals": "26.6.2",
    "@testing-library/react": "11.2.7",
    "babel-jest": "26.6.3",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "css-loader": "~5.2.6",
    "css-minimizer-webpack-plugin": "~3.0.0",
    "dotenv-webpack": "~7.0.2",
    "eslint": "~7.27.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.23.3",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-react": "~7.23.2",
    "eslint-plugin-react-hooks": "~4.2.0",
    "html-webpack-plugin": "~5.3.1",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "~1.6.0",
    "postcss-preset-env": "~6.7.0",
    "prettier": "~2.3.0",
    "react-hot-loader": "~4.13.0",
    "style-loader": "~2.0.0",
    "stylelint-config-recommended": "~5.0.0",
    "url-loader": "~4.1.1",
    "webpack": "~5.38.1",
    "webpack-bundle-analyzer": "~4.4.2",
    "webpack-cli": "~4.7.0",
    "webpack-dev-server": "~3.11.2",
    "webpack-merge": "~5.7.3"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "^(actions)/(.*)$": "<rootDir>/src/redux/$1/$2",
      "^(api)/(.*)$": "<rootDir>/src/$1/$2",
      "^(app)/(.*)$": "<rootDir>/src/$1/$2",
      "^(components)/(.*)$": "<rootDir>/src/$1/$2",
      "^(domain)/(.*)$": "<rootDir>/src/$1/$2",
      "^(images)/(.*)$": "<rootDir>/src/$1/$2",
      "^(reducers)/(.*)$": "<rootDir>/src/redux/$1/$2"
    }
  },
  "lint-staged": {
    "*.(js|html|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "^12"
  }
}
